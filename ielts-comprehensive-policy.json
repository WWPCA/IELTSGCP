{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "bedrock:InvokeModel",
                "bedrock:InvokeModelWithResponseStream",
                "bedrock:GetFoundationModel",
                "bedrock:ListFoundationModels",
                "bedrock:GetModelInvocationLoggingConfiguration",
                "bedrock:PutModelInvocationLoggingConfiguration"
            ],
            "Resource": [
                "arn:aws:bedrock:*::foundation-model/*"
            ],
            "Condition": {
                "StringEquals": {
                    "aws:username": "WWP"
                }
            }
        },
        {
            "Effect": "Allow",
            "Action": [
                "lambda:*"
            ],
            "Resource": "arn:aws:lambda:*:116981806044:function:ielts-genai-prep-*",
            "Condition": {
                "StringEquals": {
                    "aws:username": "WWP"
                }
            }
        },
        {
            "Effect": "Allow",
            "Action": [
                "iam:PassRole",
                "iam:GetRole",
                "iam:CreateRole",
                "iam:AttachRolePolicy",
                "iam:DetachRolePolicy",
                "iam:ListRoles",
                "iam:ListRolePolicies",
                "iam:ListAttachedRolePolicies",
                "iam:CreatePolicy",
                "iam:GetPolicy",
                "iam:GetPolicyVersion",
                "iam:ListPolicyVersions"
            ],
            "Resource": [
                "arn:aws:iam::116981806044:role/ielts-genai-prep-*",
                "arn:aws:iam::116981806044:policy/ielts-genai-prep-*"
            ],
            "Condition": {
                "StringEquals": {
                    "aws:username": "WWP"
                }
            }
        },
        {
            "Effect": "Allow",
            "Action": [
                "cloudwatch:*",
                "logs:*"
            ],
            "Resource": [
                "arn:aws:logs:*:116981806044:log-group:/aws/lambda/ielts-genai-prep-*",
                "arn:aws:cloudwatch:*:116981806044:*"
            ],
            "Condition": {
                "StringEquals": {
                    "aws:username": "WWP"
                }
            }
        },
        {
            "Effect": "Allow",
            "Action": [
                "dynamodb:*"
            ],
            "Resource": [
                "arn:aws:dynamodb:*:116981806044:table/ielts-genai-prep-*",
                "arn:aws:dynamodb:*:116981806044:table/ielts-genai-prep-*/index/*",
                "arn:aws:dynamodb:*:116981806044:table/ielts-genai-prep-*/stream/*"
            ],
            "Condition": {
                "StringEquals": {
                    "aws:username": "WWP"
                }
            }
        },
        {
            "Effect": "Allow",
            "Action": [
                "application-autoscaling:*",
                "autoscaling:*"
            ],
            "Resource": "*",
            "Condition": {
                "StringEquals": {
                    "aws:username": "WWP"
                }
            }
        },
        {
            "Effect": "Allow",
            "Action": [
                "cloudformation:*"
            ],
            "Resource": [
                "arn:aws:cloudformation:*:116981806044:stack/ielts-genai-prep-*/*",
                "arn:aws:cloudformation:*:116981806044:stackset/ielts-genai-prep-*:*"
            ],
            "Condition": {
                "StringEquals": {
                    "aws:username": "WWP"
                }
            }
        },
        {
            "Effect": "Allow",
            "Action": [
                "cloudcontrol:*"
            ],
            "Resource": "*",
            "Condition": {
                "StringEquals": {
                    "aws:username": "WWP"
                }
            }
        },
        {
            "Effect": "Allow",
            "Action": [
                "elasticache:*"
            ],
            "Resource": [
                "arn:aws:elasticache:*:116981806044:cluster:ielts-genai-prep-*",
                "arn:aws:elasticache:*:116981806044:replicationgroup:ielts-genai-prep-*",
                "arn:aws:elasticache:*:116981806044:subnetgroup:ielts-genai-prep-*"
            ],
            "Condition": {
                "StringEquals": {
                    "aws:username": "WWP"
                }
            }
        },
        {
            "Effect": "Allow",
            "Action": [
                "route53:*",
                "route53domains:*"
            ],
            "Resource": "*",
            "Condition": {
                "StringEquals": {
                    "aws:username": "WWP"
                }
            }
        },
        {
            "Effect": "Allow",
            "Action": [
                "apigateway:*"
            ],
            "Resource": [
                "arn:aws:apigateway:*::/restapis/n0cpf1rmvc/*",
                "arn:aws:apigateway:*::/domainnames/ieltsaiprep.com*",
                "arn:aws:apigateway:*::/domainnames/www.ieltsaiprep.com*"
            ],
            "Condition": {
                "StringEquals": {
                    "aws:username": "WWP"
                }
            }
        },
        {
            "Effect": "Allow",
            "Action": [
                "acm:*"
            ],
            "Resource": "*",
            "Condition": {
                "StringEquals": {
                    "aws:username": "WWP"
                },
                "StringLike": {
                    "acm:DomainName": "*.ieltsaiprep.com"
                }
            }
        },
        {
            "Effect": "Allow",
            "Action": [
                "ec2:DescribeVpcs",
                "ec2:DescribeRegions",
                "ec2:DescribeNetworkInterfaces",
                "ec2:DescribeSubnets",
                "ec2:DescribeSecurityGroups"
            ],
            "Resource": "*",
            "Condition": {
                "StringEquals": {
                    "aws:username": "WWP"
                }
            }
        }
    ]
}